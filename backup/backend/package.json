{
  "name": "easy-huawei",
  "version": "1.0.0",
  "description": "Sistema de gerenciamento para OLT Huawei MA5800-X7",
  "main": "src/index.js",
  "scripts": {
    "start": "NODE_ENV=production node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "nodemon src/index.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "test": "jest"
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.21.2",
    "node-ssh": "^13.2.1",
    "sequelize": "^6.37.7",
    "sqlite3": "^5.1.7",
    "ssh2": "^1.11.0",
    "telnet-client": "^2.1.2",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22"
  }
}
