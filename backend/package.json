{
  "name": "easy-huawei",
  "version": "1.0.0",
  "description": "Gerenciador de OLT Huawei",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "dev:frontend": "cd src/frontend && npm run dev",
    "dev:backend": "nodemon src/index.js",
    "dev:all": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\""
  },
  "dependencies": {
    "axios": "^1.6.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "telnet-client": "^2.1.2",
    "winston": "^3.12.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.0"
  }
} 